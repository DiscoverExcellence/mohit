.col-md-9.col-sm-10

  =@game.errors.full_messages unless @game.errors.empty?

  =form_for @game, html: {role:"form"} do | form |
    %br
    =form.label "Name"
    =form.text_field :name
    =form.label "Scoring Points"
    =form.text_field :scoring_points
    %br
    =form.submit class: "btn btn-default"
